{
  "name": "genesisdemo",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Assets": {
        "$path": "demo/assets"
      },
      "Dependencies": {
				"$path": "Packages"	
			}
    },

    "ServerScriptService": {
      "Server": {
        "$path": "demo/server"
      },
      "Genesis": {
        "$path": "genesis/"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "demo/client"
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true,
        "FallenPartsDestroyHeight": -4096
      },

      "Terrain": {
				"$className": "Terrain",
				"$properties": {
					"Decoration": true
				}
			}
    },
    "Lighting": {
      "$properties": {
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    },
    "Players": {
      "$properties": {
        "CharacterAutoLoads": false
      }
    }
  }
}